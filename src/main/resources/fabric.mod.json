    "Potatoboy9999"
  ],
  "contact": {
    "website": "https://www.curseforge.com/minecraft/mc-mods/fedora",
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/fedora",
    "sources": "https://github.com/PotatoPresident/Fedora",
